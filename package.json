{
  "name": "rmdb",
  "author": "gavinning <gavinning@qq.com>",
  "module": "dist/rmdb.esm.js",
  "version": "0.3.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 110,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "size-limit": [
    {
      "path": "dist/rmdb.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rmdb.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/ioredis": "^4.27.6",
    "husky": "^7.0.2",
    "size-limit": "^6.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@4a/rediskit": "^0.2.0",
    "debug": "^4.3.2",
    "ioredis": "^4.27.11",
    "regenerator-runtime": "^0.13.9"
  }
}
